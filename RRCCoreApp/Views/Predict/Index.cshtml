@model RRCCoreApp.ViewModel.NextRaceListVM

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>


<table class="table">
    <thead>
        <tr>
            <th>@Model.nextRaceList.</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.nextRaceList) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>

        </tr>
}
    </tbody>
</table>
