@model IEnumerable<RaceListService.Models.jrListItemVM>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>JR Trophy List</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.races)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.jrPoints)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.timeDiff)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.races)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.jrPoints)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.timeDiff)
        </td>

    </tr>
}

</table>
